<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zxxwl.api.article.mapper.ArticleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zxxwl.api.article.entity.Article">
        <id column="id" property="id" />
        <result column="member_id" property="memberId" />
        <result column="title" property="title" />
        <result column="cover" property="cover" />
        <result column="content" property="content" />
        <result column="share" property="share" />
        <result column="approve" property="approve" />
        <result column="comment" property="comment" />
        <result column="download" property="download" />
        <result column="pv" property="pv" />
        <result column="brief" property="brief" />
        <result column="status" property="status" />
        <result column="push_time" property="pushTime" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, member_id, title, cover, content, share, approve, comment, download, pv, brief, status, push_time, create_time, update_time
    </sql>

</mapper>
